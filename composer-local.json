{
	"name": "greenpeace/planet4-test-mars",
	"description": "Greenpeace P4 test-mars",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-master-theme": "7",
		"greenpeace/planet4-plugin-gutenberg-blocks": "dev-PLANET-7007_replace-covers-block-with-query-loop"
	},
	"data_from": "international",
	"scripts": {
		"site:custom": []
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "greenpeace/planet4-master-theme",
				"type": "wordpress-theme",
				"autoload": {
					"psr-4": {
						"P4\\MasterTheme\\": "src/"
					}
				},
				"extra": {
					"installer-paths": {
						"vendor/plugins/{$name}/": [
							"type:wordpress-plugin"
						]
					}
				},
				"version": 7,
				"dist": {
					"type": "zip",
					"url": "https://storage.googleapis.com/planet4-packages/planet4-master-theme/90f52d7945e5784791c90b2a4584d270737090b0.zip"
				}
			}
		}
	]
}
